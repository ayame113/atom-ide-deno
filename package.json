{
  "name": "atom-ide-deno",
  "main": "./lib/main",
  "version": "1.4.0",
  "description": "JavaScript and TypeScript language support with deno for Atom-IDE",
  "keywords": [
    "deno",
    "JavaScript",
    "TypeScript",
    "language server",
    "IDE"
  ],
  "repository": "https://github.com/ayame113/atom-ide-deno",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "check": "tsc --noEmit  -p ./lib/tsconfig.json",
    "format": "deno fmt --ignore=./node_modules",
    "test.format": "deno fmt --ignore=./node_modules --check",
    "lint": "deno lint --ignore=./node_modules --unstable",
    "test.lint": "deno lint --ignore=./node_modules --unstable | echo -e $(cat)",
    "test": "atom --test spec"
  },
  "dependencies": {
    "atom-languageclient": "^1.8.0",
    "atom-ts-transpiler": "^1.5.3",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/atom": "1.40.10",
    "@types/node": "^14.14.37",
    "deno-bin": "github:ayame113/deno-bin",
    "vscode-languageserver-protocol": "^3.16.0"
  },
  "atomTranspilers": [
    {
      "transpiler": "atom-ts-transpiler",
      "glob": "{!(node_modules)/**/,}*.ts?(x)"
    }
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "consumeSignatureHelp"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    },
    "debugger.provider": {
      "description": "deno debugger",
      "versions": {
        "0.0.0": "createDebuggerProvider"
      }
    }
  }
}
