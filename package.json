{
	"name": "atom-ide-deno",
	"main": "./lib/main",
	"version": "0.1.2",
	"description": "JavaScript and TypeScript language support with deno for Atom-IDE",
	"keywords": [
		"deno",
		"JavaScript",
		"TypeScript"
	],
	"repository": "https://github.com/ayame113/atom-ide-deno",
	"license": "MIT",
	"engines": {
		"atom": ">=1.0.0 <2.0.0"
	},
	"dependencies": {
		"atom-languageclient": "^1.0.7"
	},
	"configSchema": {
		"lspFlags": {
			"title": "lsp flags",
			"description": "This setting is based on [vscode extention](https://github.com/denoland/vscode_deno)",
			"type": "object",
			"order": 1,
			"properties": {
				"enable": {
					"title": "Enables language server",
					"type": "boolean",
					"default": true,
					"order": 2
				},
				"codeLens": {
					"type": "object",
					"order": 3,
					"properties": {
						"implementations": {
							"title": "Enables code lens implementations",
							"type": "boolean",
							"default": true,
							"order": 4
						},
						"references": {
							"title": "Enables code lens references",
							"type": "boolean",
							"default": true,
							"order": 5
						}
					}
				},
				"lint": {
					"title": "Enables lint",
					"type": "boolean",
					"default": true,
					"order": 6
				},
				"importMap": {
					"title": "Path to import-map",
					"description": "Relative path from the project root (ex. `./import-map/json`)",
					"type": "string",
					"default": "",
					"order": 7
				},
				"config": {
					"title": "Path to tsconfig",
					"description": "Relative path from the project root (ex. `./tsconfig.json`)",
					"type": "string",
					"default": "",
					"order": 8
				},
				"unstable": {
					"title": "Enables unstable",
					"type": "boolean",
					"default": false,
					"order": 9
				}
			}
		},
		"path": {
			"title": "Deno path",
			"description": "Path to the deno executable.（ex. `/usr/bin/deno`, `C:\\Program Files\\deno\\deno.exe`）",
			"type": "string",
			"default": "",
			"order": 10
		}
	},
	"consumedServices": {
		"linter-indie": {
			"versions": {
				"2.0.0": "consumeLinterV2"
			}
		},
		"datatip": {
			"versions": {
				"0.1.0": "consumeDatatip"
			}
		},
		"status-bar": {
			"versions": {
				"^1.0.0": "consumeStatusBar"
			}
		},
		"atom-ide-busy-signal": {
			"versions": {
				"0.1.0": "consumeBusySignal"
			}
		}
	},
	"providedServices": {
		"autocomplete.provider": {
			"versions": {
				"2.0.0": "provideAutocomplete"
			}
		},
		"code-actions": {
			"versions": {
				"0.1.0": "provideCodeActions"
			}
		},
		"code-format.range": {
			"versions": {
				"0.1.0": "provideCodeFormat"
			}
		},
		"code-highlight": {
			"versions": {
				"0.1.0": "provideCodeHighlight"
			}
		},
		"definitions": {
			"versions": {
				"0.1.0": "provideDefinitions"
			}
		},
		"find-references": {
			"versions": {
				"0.1.0": "provideFindReferences"
			}
		},
		"outline-view": {
			"versions": {
				"0.1.0": "provideOutlines"
			}
		},
		"debugger.provider": {
			"description": "deno debugger",
			"versions": {
				"0.0.0": "createDebuggerProvider"
			}
		}
	}
}
